/* Index.wxss */

.chanpin{
  overflow: hidden;
  padding-top: 25px;
  border-bottom: 5px solid #e5e4e4;
  padding-bottom: 20px;
}
.chanpin .chanpin_img{
  width: 25%;
  float: left;
  text-align: center;
}
.chanpin .chanpin_img image{
  width: 70px;
  height: 70px;
}
.chanpin text{
  color: #999a9e;
  font-size: 12px;
  text-align: center;
  display: block;
}

.title{
  border-bottom: 1px solid #e5e4e4;
  overflow: hidden;
  height: 36px;
  font-family: "华文行楷";
}
.title image{
  width: 20px;
  height: 20px;
}
.title .title_images{
    opacity: 1;
    background-color: transparent;
    border-color: rgb(34, 34, 34);
    border-radius: 0px;
    border-style: none;
    border-width: 0px;
    height: 20px;
    width: 20px;
    margin-left: 0px;
    margin-right: 0px;
    margin-top: 0px;
    position: absolute;
    left: 10px;
    top: 8px;
}
.title text{
    background-color: rgba(0, 0, 0, 0);
    border-color: rgb(34, 34, 34);
    border-style: none;
    border-width: 2px;
    color: rgb(102, 102, 102);
    font-size: 14px;
    font-weight: bold;
    height: 19px;
    line-height: 19px;
    margin-left: 0px;
    margin-top: 0px;
    opacity: 1;
    text-align: left;
    position: absolute;
    left: 40px;
    top: 9px;
    margin-right: 0px;
}
.hezuo{
  width: 100%;
  height: 500px;
  overflow: hidden;
  margin: 0 auto;
  padding-top: 15px;
  background-color: #f5f5f5;
  border-bottom: 4px solid #e5e4e4;
  }
.hezuoimg{
  width: 25%;
  height: 150px;
  float: left;
  text-align: center;
}
.hezuoimg image{
  width: 100%;
  height: 90px;
}
.hezuoimg text{
  color: #2c2c2c;
  font-size: 12px;
  font-weight: bold;
}
.liuchengdown{
  width: 100%;
}
.liuchengdown image{
  width: 100%;
}
.anli{
  width: 100%;
  margin-left:auto;
  margin-right: auto;
}
.anli_img{
  width: 100%;
}
.anli_img{
  width: 50%;
  float: left;
  margin-top: 20px;
  text-align: center;
}
.anli_img text{
  margin-bottom: 5px;
  color: #2c2c2c;
  display: block;
}
.anli_img image{
  width: 90%;
  margin-left: auto;
  margin-right: auto;
  display: block;
  
}